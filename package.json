{
  "name": "o3webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@mui/system": "^5.4.3",
    "@mui/x-data-grid": "^5.3.0",
    "@reduxjs/toolkit": "^1.7.0",
    "axios": "^0.24.0",
    "big-integer": "^1.6.51",
    "body-scroll-lock": "^4.0.0-beta.0",
    "pdfmake": "^0.2.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "^5.0.1",
    "redux": "^4.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --setupFiles './setupJest.js'",
    "eject": "react-scripts eject",
    "docs": "jsdoc -c jsdoc.conf.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/services/downloading",
      "src/App.js",
      "src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 70,
        "lines": 70
      }
    },
    "clearMocks": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.7",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "better-docs": "^2.7.2",
    "jsdoc": "^3.6.10",
    "parcel-bundler": "^1.12.5",
    "webpack": "^5.72.0"
  }
}
